{
  "manifest_version": 2,
  "name": "Web SDK Integration Previewer",
  "version": "1.0",
  "description": "This extension help me quickly preivew integration with SDK",

  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["inserter.js"]
    }
  ],

  "web_accessible_resources": [
    "init-ad.js"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
